{"code":"import { GraphQLEnumType } from 'graphql';\r\nimport { EnumError } from './error';\r\nimport { enumsRegistry } from './registry';\r\nexport { enumsRegistry } from './registry';\r\nimport { convertNativeEnumToGraphQLEnumValues } from './services';\r\nexport function registerEnum(enumDef, options) {\r\n    if (typeof options === 'string') {\r\n        options = { name: options };\r\n    }\r\n    const { name, description } = options;\r\n    if (enumsRegistry.has(enumDef)) {\r\n        throw new EnumError(name, `Enum is already registered`);\r\n    }\r\n    const values = convertNativeEnumToGraphQLEnumValues(enumDef);\r\n    const enumType = new GraphQLEnumType({\r\n        name,\r\n        description,\r\n        values,\r\n    });\r\n    enumsRegistry.set(enumDef, enumType);\r\n    return enumType;\r\n}\r\n//# sourceMappingURL=index.js.map","map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/domains/enum/index.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,eAAe,EAAE,MAAM,SAAS,CAAC;AAC1C,OAAO,EAAE,SAAS,EAAE,MAAM,SAAS,CAAC;AAEpC,OAAO,EAAE,aAAa,EAAE,MAAM,YAAY,CAAC;AAC3C,OAAO,EAAE,aAAa,EAAE,MAAM,YAAY,CAAC;AAC3C,OAAO,EAAE,oCAAoC,EAAE,MAAM,YAAY,CAAC;AAOlE,MAAM,UAAU,YAAY,CAAC,OAAe,EAAE,OAA6B;IACzE,IAAI,OAAO,OAAO,KAAK,QAAQ,EAAE;QAC/B,OAAO,GAAG,EAAE,IAAI,EAAE,OAAO,EAAE,CAAC;KAC7B;IACD,MAAM,EAAE,IAAI,EAAE,WAAW,EAAE,GAAgB,OAAO,CAAC;IAEnD,IAAI,aAAa,CAAC,GAAG,CAAC,OAAO,CAAC,EAAE;QAC9B,MAAM,IAAI,SAAS,CAAC,IAAI,EAAE,4BAA4B,CAAC,CAAC;KACzD;IAED,MAAM,MAAM,GAAG,oCAAoC,CAAC,OAAO,CAAC,CAAC;IAC7D,MAAM,QAAQ,GAAG,IAAI,eAAe,CAAC;QACnC,IAAI;QACJ,WAAW;QACX,MAAM;KACP,CAAC,CAAC;IACH,aAAa,CAAC,GAAG,CAAC,OAAO,EAAE,QAAQ,CAAC,CAAC;IACrC,OAAO,QAAQ,CAAC;AAClB,CAAC\"}","dts":{"name":"/Users/adampietrasiak/dev/oss/typegql/domains/enum/index.d.ts","writeByteOrderMark":false,"text":"import { GraphQLEnumType } from 'graphql';\r\nexport { enumsRegistry } from './registry';\r\nexport interface EnumOptions {\r\n    name: string;\r\n    description?: strin